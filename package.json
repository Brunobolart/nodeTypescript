{
  "name": "api-rest-node-typescript",
  "version": "1.0.0",
  "description": "Api rest com node e typescript",
  "main": "index.js",
  "author": "Bruno Barros",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "ts-node-dev ./src/index.ts",
    "test": "jest",
    "knex:migrate": "yarn knex --knexfile ./src/server/database/knex/Environment.ts migrate:latest",
    "knex:rollback": "yarn knex --knexfile ./src/server/database/knex/Environment.ts migrate:rollback",
    "knex:unlock": "yarn knex --knexfile ./src/server/database/knex/Environment.ts migrate:unlock",
    "knex:rollback-all": "yarn knex --knexfile ./src/server/database/knex/Environment.ts migrate:rollback --all",
    "knex:list": "yarn knex --knexfile ./src/server/database/knex/Environment.ts migrate:list",
    "knex:latest": "yarn knex --knexfile ./src/server/database/knex/Environment.ts migrate:latest",
    "knex:up": "yarn knex --knexfile ./src/server/database/knex/Environment.ts migrate:up",
    "knex:down": "yarn knex --knexfile ./src/server/database/knex/Environment.ts migrate:down",
    "knex:seed": "yarn knex --knexfile ./src/server/database/knex/Environment.ts seed:run"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "jest": "^29.3.1",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.0",
    "moment": "^2.29.4",
    "mysql": "^2.18.1",
    "sqlite3": "^5.1.4",
    "ts-jest": "^29.0.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^29.2.5",
    "@types/moment": "^2.13.0",
    "@types/mysql": "^2.15.21",
    "@types/node-localstorage": "^1.3.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "node-localstorage": "^2.2.1",
    "supertest": "^6.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
